{% extends 'base.html.twig' %}
{% block title %}Modifier  Depence | BusinessPark{% endblock %}
{% block content %}
    <section id="page-title">
    
                            <div class="row">
                                <div class="col-sm-8">
                                    <h1 class="mainTitle">Gestion de depence </h1>
                                    
                                </div>
                                <ol class="breadcrumb">
                                    <li>
                                        <span>gestion des depences</span>
                                    </li>
                                    <li class="active">
                                        <span>Modifier</span>
                                    </li>
                                </ol>
                            </div>
                        </section>
                     <div class="container-fluid container-fullw bg-white">
                            <div class="row">
                                <div class="col-md-12">
                                    <h5 class="over-title margin-bottom-15"> Modifier  <span class="text-bold">Depence</span></h5>
                                    
                                    <div class="row">
                                        <div class="col-md-12">
                                            {{ form_start(form) }}
                                            {{ form_errors(form) }}
                                          
                                            <fieldset>
                                                <legend>
                                                    Categorie
                                                </legend>
                                                <div class="form-group">
                                                    <label>
                                                        Intitule <span class="symbol required"></span>
                                                    </label>
                                                    <div class="form-group">
                                                        
                                                        {{ form_widget(form.fournisseur,{ 'attr': {'class': 'form-control'} }) }}
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label>
                                                    Valeur <span class="symbol required"></span>
                                                    </label>
                                                    <div class="form-group">
                                                        
                                                        {{ form_widget(form.valeur,{ 'attr': {'class': 'form-control','placeholder':'tappez l email'} }) }}
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label>
                                                        Type  <span class="symbol required"></span>
                                                    </label>
                                                    <div class="form-group">
                                                        
                                                        {{ form_widget(form.type,{ 'attr': {'class': 'form-control','placeholder':'tappez l email'} }) }}
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label>
                                                        Date DÃ©pence <span class="symbol required"></span>
                                                    </label>
                                                    <div class="form-group">
                                                        
                                                        {{ form_widget(form.datedepence,{ 'attr': {'class': 'form-control'} }) }}
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label>
                                                        Remarque <span class="symbol required"></span>
                                                    </label>
                                                    <div class="form-group">
                                                        
                                                        {{ form_widget(form.remarque,{ 'attr': {'class': 'form-control'} }) }}
                                                    </div>
                                                </div>
                                                {{ form_widget(form.save,{ 'attr': {'class': 'btn btn-o btn-primary'} }) }}
                                                        {{ form_end(form) }} 
                                            </fieldset>
                                            
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

{% endblock %}
